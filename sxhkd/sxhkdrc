super + space
      rofi -show drun

XF86AudioRaiseVolume
      volumecontrol up

XF86AudioLowerVolume
      volumecontrol down

XF86AudioMute
      volumecontrol mute

XF86AudioMicMute
      pactl set-source-mute @DEFAULT_SOURCE@ toggle 

XF86MonBrightnessUp
      brightnesscontrol up

XF86MonBrightnessDown
      brightnesscontrol down
super + l
      i3lock -i $HOME/.config/xautolock/lockscreen.png 

super + m
      ambients
super + p
      powerscript

super + c
      rofi -modi "clipboard:greenclip print" -show clipboard -run-command '{cmd}'

super + b
      zen-browser

super + e
      $TERMINAL nvim

super + shift + i
      $TERMINAL sysinfo

super + shift + w
      wallpaper

super + t
	notify-send -t 2000  $(date '+%H:%M')

super + Tab
  rofi -show window -theme-str "inputbar \{ enabled: false; \}"
